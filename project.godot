[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=4
window/size/resizable=false

[dotnet]

project/assembly_name="Zelda-2D"

[file_customization]

folder_colors={
"res://Prefabs/": "blue",
"res://Scenes/": "pink",
"res://Scripts/": "purple"
}

[rendering]

textures/canvas_textures/default_texture_filter=0
